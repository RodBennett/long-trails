{{#each campData as |data|}}

<div id="campsite-header" class="row">
    <h3>{{ nameState }}</h3>
</div>
<div class="row">

    {{!MAIN PHOTO}}
    <img id="campsite-photo" src="{{ images.[0].url }}" alt="{{ images.[0].altText }}">
</div>
<div class="row">
    <h3 class="campstie-name"> {{ name }} </h3>
    <p id="campsite-desc">{{ description }}</p>
</div>
<div class="row">
    <div class="column">
        {{!RESERVATIONS}}
        
        <p>Reservation Info: {{ reservationInfo }}</p>
        <p>Make a reservation: <a href="{{ reservationUrl }}" target="_blank">{{ reservationUrl }}</p></a>
        <br>
        <br>
        {{!REGULATIONS}}
        <p>Regulations: {{ regulationsOverview }}</p><br>
            {{!-- <p class="regulations">{{ regulationsurl }}"</p> --}}

    </div>
    <div class="column">
        {{!CONTACT}}
        <p>Phone: {{ contacts.phoneNumbers.[0].phoneNumber }}</p>
        <p>Email: <span><a href="mailto:{{contacts.emailAddresses.[0].emailAddress}}">{{ contacts.emailAddresses.[0].emailAddress}}</a></span></p>
        
        <br>
        <br>
        {{!FEES}}
        <p>Fees: {{fees.[0].title}} ${{fees.[0].cost}}: {{fees.[0].description}}</p>
         {{/each}} 
    </div>
</div>


